<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PictureLibrary.MIUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PictureLibrary.MIUI"
    xmlns:fileExplorer="clr-namespace:PictureLibrary.FileExplorer.UI;assembly=PictureLibrary.FileExplorer.UI"
    xmlns:vm="clr-namespace:PictureLibrary.FileExplorer.UI;assembly=PictureLibrary.FileExplorer.UI"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Files"
        ContentTemplate="{DataTemplate fileExplorer:FileExplorerPageViewModel}"
        Route="FileExplorer" />
</Shell>
